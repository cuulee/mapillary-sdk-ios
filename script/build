#!/bin/bash

pwd
ls

cd $WORKSPACE/../MapillarySDK

echo "Building..."

xcodebuild -workspace MapillarySDK.xcworkspace \
           -scheme MapillarySDK \
           -configuration Debug \
           -destination 'platform=iOS Simulator,OS=11.0,name=iPhone 6' \
           -derivedDataPath "build" \
           build-for-testing